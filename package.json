{
  "name": "iqsuite-js",
  "version": "0.1.0",
  "description": "JavaScript/TypeScript SDK for IQSuite API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "iqsuite",
    "api",
    "sdk"
  ],
  "author": "Blue Hex Software",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blue-hex/iqsuite-js.git"
  },
  "bugs": {
    "url": "https://github.com/blue-hex/iqsuite-js/issues"
  },
  "homepage": "https://github.com/blue-hex/iqsuite-js#readme",
  "dependencies": {
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}

# tsconfig.json
{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist", "**/*.test.ts"]
}